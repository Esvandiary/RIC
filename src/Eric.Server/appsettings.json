{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": true,
        "TimestampFormat": "'['yyyy-MM-dd HH:mm:ss'Z] '",
        "UseUtcTimestamp": true
      }
    }
  },
  "AllowedHosts": "*"
}
